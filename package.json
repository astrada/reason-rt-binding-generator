{
  "name": "reason-rt-binding-generator",
  "version": "0.1.0",
  "description": "Reason binding generator for react-toolbox",
  "license": "MIT",
  "repository": "https://github.com/astrada/reason-rt-binding-generator.git",
  "author": "Alessandro Strada",
  "scripts": {
    "typedoc": "typedoc -options typedoc.json -json output/react-toolbox.typedoc.json node_modules/react-toolbox/lib/",
    "refmt": "refmt --in-place output/*.re; refmt --in-place --interface=true output/ReactToolbox.rei; refmt --in-place --interface=true output/ReactToolboxBundled.rei"
  },
  "devDependencies": {
    "@types/react": "^16.4.7",
    "react-toolbox": "^2.0.0-beta.13",
    "typedoc": "^0.11.1",
    "typescript": "2.7.2"
  },
  "resolutions": {
    "react-transition-group": "2.2.1",
    "@types/prop-types": "15.5.2"
  }
}
